{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "react/prop-types": "0",
    "react/display-name": "0",
    "no-console": 1,
    "no-alert": 1,
    "no-empty": 2,
    "no-unused-vars": 2,
    "react/react-in-jsx-scope": 0,
    "react/no-string-refs": [
      true,
      {
        "noTemplateLiterals": false
      }
    ]
  }
}